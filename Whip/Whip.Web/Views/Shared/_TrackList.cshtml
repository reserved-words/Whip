@model List<Whip.Web.Models.TrackViewModel>

@if (Model != null && Model.Any())
{
    <table class="table table-striped table-condensed table-bordered">
        <tbody>
            @foreach (var track in Model)
                {
                <tr>
                    <td>@Html.DisplayFor(m => track.Track.Title)</td>
                    <td>@Html.DisplayFor(m => track.Track.Artist.Name)</td>
                    <td>@Html.DisplayFor(m => track.Track.Disc.Album.Title)</td>
                    <td>@Html.DisplayFor(m => track.Track.Disc.Album.Year)</td>
                </tr>
            }

        </tbody>
    </table>
}
else
{
    <p>No tracks to display</p>
}